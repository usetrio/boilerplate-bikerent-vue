{
  "name": "boilerplate-bikerent-vue",
  "version": "1.0.0",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "npm run build --force && vite preview",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test:unit:snapshots": "vitest -u run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "NODE_ENV=production eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:staged": "NODE_ENV=production eslint --fix",
    "deploy": "echo 'Deploy task unavailable' && exit 1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.2.0",
    "axios": "1.1.3",
    "lodash-es": "4.17.21",
    "normalize.css": "8.0.1",
    "pinia": "2.0.23",
    "vue": "3.2.41",
    "vue-meta": "3.0.0-alpha.10",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@rushstack/eslint-patch": "1.2.0",
    "@snug/core": "1.0.0-rc.6",
    "@snug/modules": "1.0.0-rc.3",
    "@types/jsdom": "20.0.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.68",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/coverage-c8": "0.25.7",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.2.0",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.13",
    "cypress": "10.11.0",
    "eslint": "8.26.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-vue": "9.6.0",
    "husky": "8.0.1",
    "jsdom": "20.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss-discard-comments": "5.1.2",
    "postcss-extend-rule": "4.0.0",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "sass-bem-mixins": "1.1.7",
    "start-server-and-test": "1.14.0",
    "typescript": "4.8.4",
    "vite": "3.2.0",
    "vitest": "0.24.3",
    "vue-tsc": "1.0.9"
  }
}
